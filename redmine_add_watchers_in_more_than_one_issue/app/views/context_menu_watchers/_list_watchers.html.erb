<%watchers.each do |user|%>
  <li id="<%=user.id%>">
    <%=link_to_user(user, :class => 'user')%>
    <%if @allow_delete%>
      <%=link_to image_tag('delete.png'), context_menu_watchers_path(ids: issues.map(&:id), user_id: user.id), :remote => true, :method => 'delete', :class => "delete" %>
    <%end%> 
  </li>
<%end%>
